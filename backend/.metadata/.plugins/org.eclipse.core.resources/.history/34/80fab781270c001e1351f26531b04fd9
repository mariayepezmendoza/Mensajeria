package com.maria.api.impl.service;

import java.util.List;

import com.maria.api.model.Mensaje;

import io.reactivex.Observable;

/**
 * @author Maria
 *
 */
public interface MensajeService {

	List<Mensaje> registerMessage(List<Mensaje> message);

	Observable<Mensaje> updateMessage(Mensaje message);

	List<Mensaje> getAllMessage();

	Observable<Mensaje> getMessageById(long messageId);

	Observable<String> deleteMessage(long messageId);
}
