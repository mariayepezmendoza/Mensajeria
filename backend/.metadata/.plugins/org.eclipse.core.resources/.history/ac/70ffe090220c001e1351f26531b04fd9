package com.maria.api.impl.service;

import java.util.List;

import com.maria.api.model.Terminal;

import io.reactivex.Observable;

/**
 * @author Maria
 *
 */
public interface RelationService {

	List<Terminal> createRelation(List<Terminal> Relation);

	Observable<Terminal> updateRelation(Terminal Relation);

	List<Terminal> getAllRelation();

	Observable<Terminal> getRelationById(long RelationId);

	void deleteRelation(long RelationId);

	Observable<Terminal> getRelationByCurrency(Terminal Relation);
}
